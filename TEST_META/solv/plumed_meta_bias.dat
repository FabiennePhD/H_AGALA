dh_1_2: TORSION ATOMS=35,37,41,45 # phi 2

#PRINT ARG=dh_1_1,dh_1_2 FILE=phi_deprot.out STRIDE=500
#FLUSH STRIDE=500

# 2. ***deprot*** psi C1`-O4-C4-C5
