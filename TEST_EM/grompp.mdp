;; define = -DPOSRES 

integrator      = steep           ; Algorithm (steep = steepest descent minimization)
emtol           = 10.0        ; Stop minimization when the maximum force < 1000.0 kJ/mol/nm
emstep          = 0.01      ; Energy step size
nsteps          = 50000         ; Maximum number of (minimization) steps to perform
nstxout         = 1

;integrator               = md
;tinit                    = 0
;dt                       = 0.002
;nsteps                   = 50000        
;nstxout                  = 0

init_step                = 0
comm-mode                = Linear
nstcomm                  = 1
comm-grps                =
nstvout                  = 0
nstfout                  = 0
nstlog                   = 2000 
nstenergy                = 2000 
nstxout-compressed       = 2000
compressed-x-precision   = 1000
xtc-grps                 = 
energygrps               = 
nstcalcenergy            = 1
cutoff-scheme            = verlet
nstlist                  = 40
ns_type                  = grid
pbc                      = xyz
ewald-geometry           = 3d
rlist                    = 0.95
coulombtype              = pme
rcoulomb                 = 0.95
pme_order                = 4
fourierspacing           = 0.1
vdw-type                 = cutoff
vdw-modifier             = force-switch
rvdw                     = 0.95
rvdw-switch              = 0.85
DispCorr                 = EnerPres
table-extension          = 1
Tcoupl                   = v-rescale
tau_t                    = 1.0
ld-seed                  = -1
ref_t                    = 300
tc-grps                  = system
;annealing                = single single
;annealing-npoints        = 3 3
;annealing-time           = 0 5000 20000 0 5000 20000
;annealing-temp           = 300 300 230 300 300 230
gen_vel                  = no 
gen_temp                 = 300
gen_seed                 = -1
pcoupl                   = berendsen
pcoupltype               = isotropic
tau-p                    = 4.0
compressibility          = 4.5e-5 
ref-p                    = 1.0
constraints              = hbonds
constraint-algorithm     = lincs
lincs-order              = 4
lincs-iter               = 1
continuation             = no 
;freezegrps               = fixed 
;freezedim                = Y Y Y
